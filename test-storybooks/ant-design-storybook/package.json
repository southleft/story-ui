{
  "name": "ant-design-storybook",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "story-ui": "story-ui start --port 4002",
    "storybook-with-ui": "concurrently \"npm run storybook\" \"npm run story-ui\"",
    "generate-stories": "node ../../scripts/generate-stories-batch.js ant-design 8",
    "build-for-production": "npm run generate-stories && npm run build-storybook",
    "vercel-build": "npm run build-for-production"
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@tpitre/story-ui": "file:../..",
    "antd": "^5.26.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@storybook/addon-docs": "^9.0.15",
    "@storybook/react-vite": "^9.0.15",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.2",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-storybook": "^9.0.15",
    "globals": "^16.2.0",
    "storybook": "^9.0.15",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^7.0.0"
  }
}
