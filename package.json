{
  "name": "@tpitre/story-ui",
  "version": "1.0.0",
  "description": "AI-powered Storybook story generator for any React component library",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "story-ui": "dist/cli/index.js"
  },
  "files": [
    "dist",
    "templates",
    ".env.sample",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/mcp-server/index.js",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build",
    "test": "echo \"Tests coming soon\" && exit 0",
    "release": "semantic-release",
    "release:dry-run": "semantic-release --dry-run"
  },
  "keywords": [
    "storybook",
    "ai",
    "react",
    "components",
    "ui",
    "design-system",
    "claude",
    "mcp",
    "story-generation"
  ],
  "author": "Story UI Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/southleft/story-ui.git"
  },
  "bugs": {
    "url": "https://github.com/southleft/story-ui/issues"
  },
  "homepage": "https://github.com/southleft/story-ui#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "node-fetch": "^2.6.7",
    "commander": "^11.0.0",
    "chalk": "^5.3.0",
    "inquirer": "^9.2.0",
    "concurrently": "^8.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.4.2",
    "@types/inquirer": "^9.0.0",
    "@types/cors": "^2.8.17",
    "@types/node-fetch": "^2.6.12",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "semantic-release": "^24.2.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.3.5",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "@storybook/react": ">=6.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "@storybook/react": {
      "optional": false
    }
  }
}
