{
  "name": "@tpitre/story-ui",
  "version": "2.1.5",
  "description": "AI-powered Storybook story generator with dynamic component discovery",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "story-ui": "dist/cli/index.js"
  },
  "files": [
    "dist",
    "templates",
    ".env.sample",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/mcp-server/index.js",
    "mcp": "yarn build && node dist/mcp-server/mcp-stdio-server.js",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build",
    "test": "echo \"Tests coming soon\" && exit 0",
    "release": "semantic-release",
    "release:dry-run": "semantic-release --dry-run",
    "commit": "cz",
    "prepare": "husky"
  },
  "keywords": [
    "storybook",
    "ai",
    "react",
    "components",
    "ui",
    "design-system",
    "claude",
    "mcp",
    "story-generation",
    "documentation",
    "real-time",
    "component-library",
    "iteration",
    "version-history"
  ],
  "author": "Story UI Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/southleft/story-ui.git"
  },
  "bugs": {
    "url": "https://github.com/southleft/story-ui/issues"
  },
  "homepage": "https://github.com/southleft/story-ui#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@modelcontextprotocol/sdk": "^0.5.0",
    "@mui/material": "^7.2.0",
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "glob": "^11.0.3",
    "inquirer": "^9.2.0",
    "node-fetch": "^2.6.7",
    "typescript": "^5.8.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.3.5",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/glob": "^8.1.0",
    "@types/inquirer": "^9.0.0",
    "@types/node": "^20.4.2",
    "@types/node-fetch": "^2.6.12",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^9.1.7",
    "semantic-release": "^24.2.0",
    "ts-node": "^10.9.2"
  },
  "peerDependencies": {
    "@storybook/react": ">=6.0.0",
    "react": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "@storybook/react": {
      "optional": false
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
